@model IEnumerable<Finaliglesia.Models.Iglesia>

<br />
<br />
<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/0.9.0rc1/jspdf.min.js"></script>
<script>
    $( document ).ready(function() {
        

        $('#cbxIglecias').change(function () {
            
            var id = $(this).val();
            $.ajax({
                url: 'Iglesias/BusquedaPorIglecia/' + id,
                type: "GET",
                success: function (d) {
                    pdf(d);
                },
                error: function (d) {
                    alert('Error, intente nuevamente');
                }
            });

        });
        var id = $('#cbxIglecias').val()
        $.ajax({
            url: '/Iglesias/BusquedaPorIglecia/'+id,
            type: "GET",
            success: function (d) {
                pdf(d);
            },
            error: function (d) {
                alert('Error, intente nuevamente');
            }
        });


        

        function pdf(data) {



            var imgData = 'data:image/jpeg;base64,';

            var doc = new jsPDF('landscape');
            doc.addImage(imgData, 'JPEG', 15, 5, 280, 25)

            doc.setFontSize(25)
            var nom = $('#cbxIglecias').find('option:selected').text();

            doc.text(95, 45, 'Iglesia ' + '"' +nom+'"')

            doc.setFontSize(14)
            var pos = 55;
            doc.text(15, pos, 'Sacerdote')
            doc.text(100, pos, 'Sacramento')
            doc.text(180, pos, 'Fecha')
            doc.text(205, pos, 'Hora')
            doc.text(225, pos, 'Ceremonia')


            doc.setFontSize(11)

            var page = 1;
            //for (var i = 0; i < 100; i++) {
            $.each(data, function (index, element) {
                if (page == 14) {
                    doc.addPage()
                    pos = 25;
                    page = 1;
                } else {
                    page++;
                }
                pos = pos + 10;
                doc.text(15, pos, element.NombreSacerdote + " " + element.ApellidoSacerdote);
                doc.text(100, pos, element.DetalleSacramento);
                doc.text(180, pos, parseJsonDate(element.Fecha));
                doc.text(205, pos, element.Hora);
                doc.text(225, pos, element.Detalle);
            });
            //}

            $("#pdf_preview").attr("src", doc.output('datauristring'));

        }

        function parseJsonDate(jsonDateString) {
            var dateString = jsonDateString.substr(6);
            var currentTime = new Date(parseInt(dateString));
            var month = currentTime.getMonth() + 1;
            var day = currentTime.getDate();
            var year = currentTime.getFullYear();
            var date = day + "/" + month + "/" + year;
            return date;
        }

    });

   
</script>
<select id="cbxIglecias" class="selectpicker">
    @foreach (var item in Model)
    {
        <option value="@Html.DisplayFor(modelItem => item.IglesiaID)">@Html.DisplayFor(modelItem => item.Nombre)</option>
       
    }
</select>
<br />
<br />
<iframe id="pdf_preview" type="application/pdf" src="" width="800" height="400"></iframe>


